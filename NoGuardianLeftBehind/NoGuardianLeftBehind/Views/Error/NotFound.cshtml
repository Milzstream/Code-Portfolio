

@{
    ViewBag.Title = "Not Found";
    Layout = null;
}

<!DOCTYPE html>
<html>

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>404 page | One Guardian Left Behind</title>
    <meta content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no' name='viewport'>
    @Styles.Render("~/Content/bootstrap")
    @Styles.Render("~/Content/404")
</head>

<body>
    <div id="animate" class="row">
        <div class="number">4</div>
        <div class="icon">
            <i class="livicon" data-name="pacman" data-size="105" data-c="#f6c500" data-hc="#f1b21d" data-eventtype="click" data-iteration="15"></i>
        </div>
        <div class="number">4</div>

    </div>
    <div class="hgroup">
        <h1>Page Not Found</h1>
        <h2>It seems that page you are looking for no longer exists.</h2>
        <a href="@Url.Action("Index", "Home")">
            <button type="button" class="btn btn-responsive button-alignment">Home</button>
        </a>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/livicons")
    @Scripts.Render("~/bundles/holder")
    @Scripts.Render("~/bundles/404")

</body>
</html>
