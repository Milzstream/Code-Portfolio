    <BlogPostRepository id="1071" key="d6d2ee4d-85ee-44c4-976a-b193c53ef4fa" parentID="1059" level="2" creatorID="0" sortOrder="3" createDate="2016-05-03T18:39:33" updateDate="2016-05-13T09:49:37" nodeName="News" urlName="news" path="-1,1059,1071" isDoc="" nodeType="1055" creatorName="Shadeslayor" writerName="Shadeslayor" writerID="0" template="1050" nodeTypeAlias="BlogPostRepository">
